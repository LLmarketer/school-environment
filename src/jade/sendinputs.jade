include ../mixins/head
body
    nav.navbar.navbar-inverse
        .container-fluid
            .navbar-header
                a.navbar-brand(href="#") Title company

            .collapse.navbar-collapse
                ul.nav.navbar-nav
                    li
                    li
                        a(href="#") Home
                    li.active
                        a(href="#") Items
                    li
                        a(href="#") About
                    li
                        a(href="#") Contact

    .content
        .col-lg-10
            form.form
                .form-group
                    label(for="name") Name
                    input.form-control(type="text" id="name" placeholder="Your name" name="name")
                .form-group
                    label(for="email") Email
                    input.form-control(type="email" id="email" placeholder="name@example.com" name="email")
                button.btn.btn-default.send-email(type="submit") Subscription

            .subcriptors
                table.table
                    thead
                        tr
                            th Name
                            th Email
                    tbody


    include ../mixins/scripts

